{
    "name": "dicord-bot",
    "version": "1.0.0",
    "description": "Harness the power of ChatGPT on Discord with Nisa. Engage in real-time chats or delve into personal finance queries. Integrated with Plaid's API, ask about your bank transactions and get swift, precise responses. Prioritizing safety, it ensures encrypted interactions without storing personal data.",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
        "build": "tsc",
        "test": "jest",
        "register": "tsc && node dist/register_commands/register.js",
        "generate": "tsc && node dist/generate_template/generate.js",
        "deploy": "tsc && sam build --template-file dist/template.yaml && sam deploy --guided --template-file dist/template.yaml"
    },
    "keywords": [
        "Discord Bot",
        "Discord",
        "Bot",
        "Financial Advisor"
    ],
    "author": "cyvid7-darus10",
    "private": true,
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^1.6.5",
        "@types/jest": "^29.5.4",
        "aws-sdk": "^2.1457.0",
        "axios": "^1.5.0",
        "discord.js": "^14.13.0",
        "dotenv": "^16.3.1",
        "jest": "^29.7.0",
        "openai": "^4.6.0",
        "path": "^0.12.7",
        "require-all": "^3.0.0",
        "ts-jest": "^29.1.1",
        "tweetnacl": "^1.0.3",
        "yaml": "^2.3.2"
    },
    "devDependencies": {
        "@types/node": "^20.6.0",
        "@types/yargs": "^17.0.24",
        "prettier": "3.0.3",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.2.2",
        "yaml-cfn": "^0.3.2"
    }
}
